{
    "homepage": "https://Douglas1705.github.io/Projeto-Zephyr",
    "name": "zephyr",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"vite\" \"json-server --watch db.json --port 3001\"",
        "build:css": "tailwindcss -i ./src/index.css -o ./dist/assets/index.css",
        "build": "vite build && npm run build:css && tsc -b",
        "lint": "eslint .",
        "preview": "vite preview",
        "watch:css": "tailwindcss -i ./src/index.css -o ./dist/styles.css --watch",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "eject": "react-scripts eject",
        "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    },
